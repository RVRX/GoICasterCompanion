<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import java.lang.Integer?>
<AnchorPane prefHeight="275.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="goistreamtoolredux.controller.Default">
   <children>
      <VBox prefHeight="275.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="43.0" prefWidth="600.0">
               <children>
                  <JFXComboBox fx:id="TeamAComboBox" onAction="#selectTeamA" prefHeight="27.0" prefWidth="168.0" promptText="Team A" />
                  <JFXComboBox fx:id="TeamBComboBox" onAction="#selectTeamB" prefHeight="27.0" prefWidth="168.0" promptText="Team B" />
               </children>
            </HBox>
            <HBox prefHeight="43.0" prefWidth="600.0">
               <children>
                  <JFXComboBox fx:id="mapComboBox" onAction="#selectMap" prefHeight="27.0" prefWidth="124.0" promptText="Map Selection" />
                  <Spinner fx:id="TournamentNumberSpinner" amountToStepBy="1" editable="true" initialValue="1" min="1" prefHeight="27.0" prefWidth="79.0">
                     <max>
                        <Integer fx:constant="MAX_VALUE" />
                     </max>
                  </Spinner>
                  <JFXButton fx:id="saveButton" onAction="#save" text="Save" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="teamAImage" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <ImageView fx:id="mapImage" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <ImageView fx:id="teamBImage" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <JFXButton fx:id="lobbyTimerButton" onAction="#lobbyTimerAction" text="Lobby Timer" />
                     </children></VBox>
               </children>
            </HBox>
            <Label fx:id="testLabel" text="Label" />
         </children>
      </VBox>
   </children>
</AnchorPane>
